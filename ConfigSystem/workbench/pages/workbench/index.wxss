}

.line {
  width: 100%;
  height: 15rpx;
  margin-top: 45rpx;
  background-color: #e4e4e4;
  border: #d7d7d7 solid 3rpx;
}

.analyze {
  display: flex;
  width: 90%;
  height: 300rpx;
  margin-left: auto;
  margin-right: auto;
  margin-top: 15rpx;
}

.analyzecolmn {
  width: 49%;
