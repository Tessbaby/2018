.main {
  width: 100%;
  background-color: #333;
  height: 200rpx;
}

.mainline {
  display: flex;
  width: 90%;
  height: 200rpx;
  margin-left: auto;
  margin-right: auto;
}

.maingridcolmn {
  width: 50%;
  height: 200rpx;
}

.maingridimg {
  width: 80rpx;
  height: 80rpx;
