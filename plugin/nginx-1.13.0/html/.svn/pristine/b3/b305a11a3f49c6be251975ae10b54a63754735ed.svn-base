/*时间轴*/
.time-line-wrap{position:relative;border-left:2px solid #c6c6c6;padding-bottom:15px;}
.time-dot{position:absolute;top:0;left:-6px;width:10px;height:10px;background:#c6c6c6;border-radius:50%;}
.time-dot.active{position:absolute;top:0px;left:-11px;width:20px;height:20px;background:#b6e444;border-radius:50%;}
.time-dot.active::before{content:'';position:absolute;top:4px;left:4px;width:12px;height:12px;background:#92c80e;border-radius:50%;}
.time-content{position:relative;margin-left:30px;margin-top:-6px;}
.time-line-wrap:last-of-type{border-color:transparent;}

