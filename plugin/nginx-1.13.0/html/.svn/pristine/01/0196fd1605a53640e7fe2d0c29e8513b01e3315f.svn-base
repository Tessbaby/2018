<toaster-container toaster-options="{'position-class': 'toast-center', 'close-button':true}"></toaster-container>

<div class="wrapper">
    <div>
        <ul class="breadcrumb">
            <li><a><i class="fa fa-home"></i>党委办公</a></li>
            <li class="active"><a ui-sref="main.party.hb">工作汇报</a></li>
            <li class="active"><a href="#">详情</a></li>
        </ul>
    </div>

    <div class="org-m-t20 ">
        <div class="add-content white-bg form-horizontal p-md">
            <div ng-show="data.allowDel==1">
                <a class="btn btn-info pull-right m-l-xs" ng-click="del(data.id)">
                    <i class="icon-user-follow m-r-sm text-md"></i>删除
                </a>
            </div>
            <h5>{{data.title}}</h5>
            <div class="m-t-md m-b-md">
                <span class="inline">类型：</span>
                <span class="inline m-r">{{data.typeName}}</span>

                <span class="inline">汇报时间：</span>
                <span class="inline m-r">{{data.createTime}}</span>
                <div class="inline" ng-show="data.allowDel!=1">
                    <span class="inline">汇报者：</span>
                    <span class="inline m-r">{{data.reportDeptName}}</span>

                </div>
                <div class="inline" ng-show="data.allowDel==1">
                    <span class="inline">上级阅读状态：</span>
                    <span class="inline m-r" ng-show="data.status==1">未读</span>
                    <span class="inline m-r" ng-show="data.status==2">已读</span>
                </div>
            </div>
            <div>
                {{data.content}}
            </div>





